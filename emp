class Employee:
    def __init__(self, name, years_worked):
        self.name = name
        self.years_worked = years_worked

def transfer_employees_to_backup(employees):
    backup_employees = []
    remaining_employees = []  # to store employees remaining on main servers
    for employee in employees:
        if employee.years_worked >= 10:
            backup_employees.append(employee)
            print(f"–ù”©”©—Ü —Å–µ—Ä–≤–µ—Ä–ª“Ø“Ø {employee.name}-–∏–π–Ω –º—ç–¥—ç—ç–ª–∏–π–≥ —à–∏–ª–∂“Ø“Ø–ª–∂ –±–∞–π–Ω–∞...")
        else:
            remaining_employees.append(employee)
    return backup_employees, remaining_employees

def main():
    employees = [
        Employee("–ë–∞—Ç", 12),
        Employee("–ë–æ–ª–¥", 7),
        Employee("–ë—É—è–Ω–∞–∞", 15),
        Employee("–ê–Ω—Ö–∞–∞", üòé,
        Employee("–¢”©”©–≥”©”©", 10),
        Employee("–ë–∏–ª–≥“Ø“Ø–Ω—ç—ç", 1),
        Employee("–ë–∞–∑—Ä–∞–∞", 17),
        Employee("–≠—Ä–¥—ç–Ω—ç", 20),
    ]

    print("–£–ë “Æ–Ω–¥—Å—ç–Ω —Å–µ—Ä–≤–µ—Ä –¥—ç—ç—Ä –±–∞–π–≥–∞–∞ –∞–∂–∏–ª—á–∏–¥:")
    print("+----------------+------------+")
    print("|     –ù—ç—Ä        |   –ê–∂–∏–ª–ª–∞—Å–∞–Ω –ñ–∏–ª|")
    print("+----------------+------------+")
    for employee in employees:
        print(f"| {employee.name:<14}| {employee.years_worked:^10}|")
    print("+----------------+------------+")

    backup_employees, remaining_employees = transfer_employees_to_backup(employees)

    print("\n–î–∞—Ä—Ö–∞–Ω –ù”©”©—Ü —Å–µ—Ä–≤–µ—Ä—Ç –±–∞–π–≥–∞–∞ –∞–∂–∏–ª—á–∏–¥ (10+ –∂–∏–ª –∞–∂–∏–ª–ª–∞—Å–∞–Ω):")
    print("+----------------+------------+")
    print("|     –ù—ç—Ä        |   –ê–∂–∏–ª–ª–∞—Å–∞–Ω –ñ–∏–ª|")
    print("+----------------+------------+")
    for employee in backup_employees:
        print(f"| {employee.name:<14}| {employee.years_worked:^10}|")
    print("+----------------+------------+")

    print("\n“Æ–Ω–¥—Å—ç–Ω —Å–µ—Ä–≤–µ—Ä –¥—ç—ç—Ä “Ø–ª–¥—Å—ç–Ω –∞–∂–∏–ª—á–∏–¥:")
    print("+----------------+------------+")
    print("|     –ù—ç—Ä        |   –ê–∂–∏–ª–ª–∞—Å–∞–Ω –ñ–∏–ª|")
    print("+----------------+------------+")
    for employee in remaining_employees:
        print(f"| {employee.name:<14}| {employee.years_worked:^10}|")
    print("+----------------+------------+")

if _name_ == "_main_":
    main()
